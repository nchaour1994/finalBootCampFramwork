<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="practice" parallel="false">
    <parameter name="useCloud" value="false"></parameter>
    <parameter name="cloudEnvName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="versionOs" value="10"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="browserVersion" value="99"></parameter>
    <parameter name="URL" value="https://www.walgreens.com/"></parameter>


    <test name="access to services">
        <classes>
            <class name="org.piit.accessToHealthProfile.AccessToHealthProfile"></class>
            <class name="org.piit.accessToPrescriptionStatus.AccessToPrescription"></class>
            <class name="org.piit.accessToMessages.AccessToMessage"></class>
            <class name="org.piit.accessToEmailAndAlert.accessToEmailAndAlert"></class>
            <class name="org.piit.accessShoppingListAndBuyItAgain.AccessToShoppingList"></class>
            <class name="org.piit.accessToMyShoppingCart.AccessShoppingCart"></class>
            <class name="org.piit.acccessToCoupon.AccessToCoupon"></class>
            <class name="org.piit.accessWeeklyad.WeekyAd"></class>
            <class name="org.piit.accessToPhotoDeal.PhotoDeal"></class>
            <class name="org.piit.accessToHealthInssuranceMarketPlace.healthIssuranceMarketPlace"></class>
            <class name="org.piit.acccessToCoupon.AccessToCoupon"></class>
            <class name="org.piit.accessToShopAllPhoto.AccessToAllPhoto"></class>

        </classes>
    </test>
    <test name="Schedule appointments">
        <classes>
            <class name="org.piit.transferRx.scheduleFluVaccine.ScheduleFluVaccine"></class>
            <class name="org.piit.scheduleBoosterShot.ScheduleBoosterShot"></class>
            <class name="org.piit.appointmentsForCovid.ScheduleAppointment"></class>


        </classes>
    </test>
    <test name="login and logout ">
        <classes>
            <class name="org.piit.login.LoginToAccount"></class>
            <class name="org.piit.logout.Logout"></class>
        </classes>
    </test>
    <test name="make cards and photos ">
        <classes>
            <class name="org.piit.takePhotos.MakePhotoBook"></class>
            <class name="org.piit.orderPostCard.OrderBusinessPostCard"></class>
            <class name="org.piit.makeCanvasAndDecor.MakeCanvas"></class>
            <class name="org.piit.makeCalendar.MakeCalendar"></class>
            <class name="org.piit.takePhotos.MakePhotoBook"></class>
            <class name="org.piit.makeCanvasAndDecor.MakeCanvas"></class>
            <class name="org.piit.makePoster.MakePoster"></class>
            <class name="org.piit.orderPostCard.OrderBusinessPostCard"></class>
            <class name="org.piit.shopPassportPhoto.ShopPassportPhotos"></class>
            <class name="org.piit.makeFullPhoto.MakeFullPhoto"></class>
            <class name="org.piit.makeBirthDayCard.MakeBirthDayCard"></class>
            <class name="org.piit.makeCustomPuzzle.MakeCustomPuzzle"></class>

        </classes>
    </test>
    <test name="activities">
        <classes>
            <class name="org.piit.joinMyWalgreens.JoinMyWalgreens"></class>
            <class name="org.piit.sortItems.SortItems"></class>
            <class name="org.piit.qualificationForCreditCard.QualifyForCreditCard"></class>
            <class name="org.piit.findCare.FindCare"></class>
            <class name="org.piit.chat.ChatWithWalgreens"></class>
            <class name="org.piit.orderTestCovid.OderTests"></class>
            <class name="org.piit.purchase.PurchaseItems"></class>

        </classes>
    </test>
    <test name="view services">
        <classes>
            <class name="org.piit.viewBenefitsAndActivities.viewBenefits"></class>
            <class name="org.piit.viewPurchaseHistory.ViewHistory"></class>
            <class name="org.piit.viewMemberShip.ViewMemberShip"></class>
            <class name="org.piit.findLocations.FindLocationsNearby"></class>
            <class name="org.piit.viewVaccineRecord.ViewRecordVaccine"></class>

        </classes>
    </test>
    <test name="other activities ">
        <classes>
            <class name="org.piit.printBook.PrintBook"></class>
            <class name="org.piit.shopGiftCards.ShopGoogleGiftCard"></class>
            <class name="org.piit.addQuantityInShoppingCart.AddQuantity"></class>
            <class name="org.piit.writeAReview.WriteReview"></class>
            <class name="org.piit.transferRx.TransferRx"></class>

        </classes>
    </test>
    <test name="create and update account ">
        <classes>
            <class name="org.piit.registration.CreateAccount"></class>
            <class name="org.piit.updateAccountInfo.UpdateAccountInformations"></class>

        </classes>
    </test>



</suite>